{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "OBJ to Blockbench",
  "version": "0.1.0",
  "identifier": "com.aistrom.obj2mc-app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "OBJ2MC",
        "width": 720,
        "height": 420,
        "minWidth": 640,
        "minHeight": 380,
        "resizable": true,
        "center": true,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
